{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++17 $fileName -O2 -Wall -Wextra -Wno-unknown-pragmas -DLOCAL -o a && $dira"
    },
    "editor.minimap.side": "right",
    "editor.renderLineHighlight": "all",
    "workbench.panel.defaultLocation": "right",
    "workbench.colorCustomizations": {
        "editorLineNumber.foreground": "#686868",
        "editorLineNumber.activeForeground": "#ffe600",
        "editor.selectionBackground": "#6b6b6b"
    },
    "explorer.sortOrder": "type",
    "cph.general.defaultLanguage": "cpp",
    "explorer.autoReveal": false,
    "editor.tabSize": 4,
    "vim.vimrc.enable": true,
    "files.associations": {
        "atomic": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "complex": "cpp",
        "condition_variable": "cpp",
        "exception": "cpp",
        "any": "cpp",
        "array": "cpp",
        "optional": "cpp",
        "future": "cpp",
        "mutex": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "shared_mutex": "cpp",
        "stdexcept": "cpp",
        "thread": "cpp",
        "type_traits": "cpp",
        "valarray": "cpp",
        "bitset": "cpp",
        "cstdlib": "cpp",
        "string": "cpp",
        "memory_resource": "cpp",
        "cmath": "cpp",
        "functional": "cpp",
        "rope": "cpp",
        "fstream": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "string_view": "cpp",
        "regex": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "variant": "cpp"
    },
}
